{
  "version": 2,
  "buildCommand": "cd stroop-game-main && npm run build",
  "devCommand": "cd stroop-game-main && npm run dev",
  "installCommand": "cd stroop-game-main && npm install --legacy-peer-deps",
  "framework": "nextjs",
  "regions": ["sin1"],
  "public": false,
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=60, stale-while-revalidate=600"
        }
      ]
    }
  ],
  "outputDirectory": ".next"
}